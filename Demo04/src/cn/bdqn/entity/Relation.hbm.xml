<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.bdqn.entity.Relation" table="RELATION" schema="SCOTT">
        <composite-id name="id" class="cn.bdqn.entity.Relation">
            <key-property name="proid">
                <column name="PROID" sql-type="number(4)" precision="4" not-null="true"/>
            </key-property>
            <key-property name="empid">
                <column name="EMPID" sql-type="number(4)" precision="4" not-null="true"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>