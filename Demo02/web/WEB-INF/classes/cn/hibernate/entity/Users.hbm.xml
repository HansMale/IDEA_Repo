<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.hibernate.entity.Users" table="USERS" schema="SCOTT">
        <id name="id" type="int">
            <column name="ID" sql-type="number(3)" precision="3"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>